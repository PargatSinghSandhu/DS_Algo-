#include<iostream>
#include<stdio.h>
using namespace std;


int sort(int arr[4])
{
    int largest=arr[0];
    
    for(int i=0;i<4;i++)
    {
        if(arr[i]>largest)
        largest=arr[i];
    }
    cout<<"Print Largest Array";
    cin>>largest;
    
}

int main()
{
    int arr[4]= {1,5,3,4};
   
    sort(arr[4]);

}
