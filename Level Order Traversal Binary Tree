#inlcude<iostream>
#incude<queue>
using namespace std;

struct Node
{
    int data;
    struct Node*left;
    struct Node*right;
};

void LevelOrder(Node*root)
{
    if(root==NULL)
        return;
    queue<Node*> Q;
    Q.push(root);
    while(!Q.empty())
    {
        Node*current =Q.front();
        cout<<current->data<<" ";
        if(current->left!=NULL)
        {
            Q.push(current->left);
        }
        if(current->right!=NULL)
        {
            Q.push(current->right==NULL);
        }
    }
}
