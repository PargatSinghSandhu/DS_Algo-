#include<iostream>
#include<vector>

using namespace std;

void intersection_array()
{   vector <int> vis;
    for( int i = 0; i<n1; i++)
    {
        for(int j=0; j<n2; j++)
        {
            if(arr1[i]==arr2[j]&& vis[j]==0)
            {
                push_back.arr[i];
                vis[j]==1;
                break;
            }
        }
    }
}




int main()
{
    int arr1[]={1,2,3,4,5};
    int arr2[]={2,3,4,5};
    
    intersection_array(arr1[], arr2[], n1)
}
